<%= form_with url: "/teams/#{@team.id}", method: :patch, local: true do |form|%>
  <%= form.label :name %> 
  <%= form.text_field :name %>
  <%= form.label :head_coach %>
  <%= form.text_field :head_coach %>
  <%= form.label :year_founded %>
  <%= form.text_field :year_founded %>
  <%= form.label 'Is the head coach Brazilian?'%>
  <%= form.check_box :is_brazilian %> 
  <%= form.submit "Update #{@team.name}" %>
<% end %>