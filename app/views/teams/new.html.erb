
<%= form_with url: '/teams', method: :post, local: true do |form| %>
  <%= form.label :name %> 
  <%= form.text_field :name %></br>
  <%= form.label :head_coach %>
  <%= form.text_field :head_coach %></br>
  <%= form.label :year_founded %>
  <%= form.text_field :year_founded %></br>
  <%= form.label 'Is the head coach Brazilian?'%>
  <%= form.check_box :is_brazilian %> </br>
  <%= form.submit "Create Team" %>
<% end %>